<!DOCTYPE html>
<html>
<head>
<style>
p {
    border: 2px solid black;
    outline: #4CAF50 solid 10px;
    margin: auto;    
    padding: 20px;
    text-align: center;
}
</style>
<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
<style>





.show {display: block;}
* {
  box-sizing: border-box;
}

#myInput {
  background-image: url('/css/searchicon.png');
  background-position: 10px 12px;
  background-repeat: no-repeat;
  width: 100%;
  font-size: 16px;
  padding: 12px 20px 12px 40px;
  border: 1px solid #ddd;
  margin-bottom: 12px;
}

#myUL {
  list-style-type: none;
  padding: 0;
  margin: 0;
}

#myUL li a {
  cursor : pointer;
  border: 1px solid #ddd;
  margin-top: 2px; /* Prevent double borders */
  background-color: #f6f6f6;
  padding: 12px;
  text-decoration: none;
  font-size: 18px;
  color: black;
  display: block
}

#myUL li a:hover:not(.header) {
  background-color: #eee;
}
</style>
</head>
<body>
<h2><%=userdetailss.Branchname %> office assets</h2>
<h3>address:<%=userdetailss.validAddress%></h3>

<input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names.." title="Type in a name">
<ul id="myUL">
    <%
    var i=0;
    while(assertrawb[i]){
    %>
  <div class="dropdown">
    <li ><a onclick="window.location.href='/assetdetails?address=<%=userdetailss.validAddress%>&city=<%=userdetailss.Branchname%>&assetname=<%=assertrawb[i].name%>'" > <%=assertrawb[i].name%>(units: <%=assertrawb[i].qty%>)</a></li>
     
    </div>
    <% 
    i++
      }
    %>
  </ul>

  
  
 


<script>
  function myFunction() {
      var input, filter, ul, li, a, i;
      input = document.getElementById("myInput");
      filter = input.value.toUpperCase();
      ul = document.getElementById("myUL");
      li = ul.getElementsByTagName("li");
      for (i = 0; i < li.length; i++) {
          a = li[i].getElementsByTagName("a")[0];
          if (a.innerHTML.toUpperCase().indexOf(filter) > -1) {
              li[i].style.display = "";
          } else {
              li[i].style.display = "none";
          }
      }
  }
  </script>


</body>
</html>